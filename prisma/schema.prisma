generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id     Int     @id @default(autoincrement())
  name   String
  email  String  @unique
  status Boolean @default(true)
  Posts Post[]
}

model Post {
  id Int @id @default(autoincrement())
  title String
  userID Int
  author User @relation(fields: [userID], references: [id])
}